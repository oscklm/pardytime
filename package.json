{
  "name": "pardytime",
  "main": "index.tsx",
  "version": "1.0.0",
  "scripts": {
    "dev": "APP_VARIANT=development expo start --dev-client",
    "android": "expo run:android",
    "prebuild": "APP_VARIANT=development expo prebuild -p ios",
    "db:dev": "convex dev",
    "db:deploy": "convex deploy",
    "db:dashboard": "convex dashboard",
    "ios": "APP_VARIANT=development expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "lint:fix": "expo lint --fix",
    "lint:check": "expo lint --check",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "typecheck": "tsc --noEmit",
    "postinstall": "husky"
  },
  "dependencies": {
    "@clerk/backend": "^2.0.0",
    "@clerk/clerk-expo": "^2.13.0",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.16",
    "@shopify/react-native-skia": "v2.0.0-next.4",
    "@tanstack/react-form": "^1.14.1",
    "color": "^5.0.0",
    "convex": "1.25.4",
    "convex-helpers": "^0.1.92",
