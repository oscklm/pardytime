  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "convex",
          "convex-helpers",
          "hono",
          "@tanstack/react-form",
          "neverthrow",
          "radon-ide",
          "tiny-invariant",
          "@clerk/backend",
          "color"
        ]
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn expo lint --fix",
      "yarn prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "yarn prettier --write"
    ]
  },
  "private": true,
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
